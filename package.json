{
  "name": "hack-fit-tracker",
  "version": "1.0.1",
  "private": true,
  "dependencies": {
    "@commitlint/cli": "^8.3.6",
    "@commitlint/config-conventional": "^8.3.6",
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@mui/material": "^5.3.0",
    "@mui/x-data-grid": "^5.3.0",
    "@storybook/addon-actions": "^5.3.21",
    "@storybook/addon-knobs": "^5.3.21",
    "@storybook/addon-links": "^5.3.21",
    "@storybook/addons": "^5.3.22",
    "@storybook/preset-create-react-app": "^1.5.2",
    "@storybook/react": "^5.3.21",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "@types/jest": "^24.9.1",
    "@types/node": "^12.20.41",
    "@types/react": "^16.14.21",
    "@types/react-dom": "^16.9.14",
    "axios": "^0.25.0",
    "commitizen": "^4.2.4",
    "cra-shared-ui-scripts": "^1.8.0",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "date-fns": "^2.28.0",
    "formik": "^2.2.9",
    "jest-canvas-mock": "^2.3.1",
    "jest-trx-results-processor": "^2.2.0",
    "mini-css-extract-plugin": "^2.5.2",
    "mkdirp": "^1.0.4",
    "move-cli": "^1.2.1",
    "react": "^17.0.2",
    "react-content-loader": "^6.1.0",
    "react-dom": "^17.0.2",
    "react-router-dom": "5.2.0",
    "react-scripts": "^5.0.0",
    "recharts": "^2.1.8",
    "standard-version": "^7.1.0",
    "typescript": "^3.7.7",
    "yup": "^0.32.11"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "version:bump": "standard-version",
    "build:ui": "yarn prep-shared-ui && react-scripts build && yarn finalize-shared-ui",
    "test:ci": "cross-env CI=true react-scripts test --env=jsdom --reporters=default --reporters=jest-trx-results-processor",
    "test:watch": "react-scripts test --env=jsdom",
    "storybook": "start-storybook -p 9009 -s public",
    "storybook:build": "build-storybook -s public",
    "commit": "git-cz"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.ts",
      "src/**/*.tsx",
      "!src/**/*.stories.tsx",
      "!src/**/*.d.ts"
    ],
    "moduleNameMapper": {
      "\\.(css|less|scss|sass)$": "identity-obj-proxy"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@types/react-router-dom": "^5.3.2",
    "node-sass": "^7.0.1"
  }
}
